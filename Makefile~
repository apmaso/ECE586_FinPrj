########################################################################

CC= gcc
CFLAGS= -Wall
LDFLAGS= -lm
OBJFILES= main.o parse.o
TARGET=main

all: $(TARGET)

$(TARGET): $(OBJFILES)
	$(CC) $(CFLAGS) -o $(TARGET) $(OBJFILES) $(LDFLAGS)

clean:
	rm -f $(OBJFILES) $(TARGET) *~
